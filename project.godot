; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="gameName3"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.png"

[audio]

mix_rate=8000
output_latency=1000
video_delay_compensation_ms=500

[display]

window/size/width=1600
window/size/resizable=false
window/size/borderless=true
window/size/always_on_top=true
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/stretch/shrink=7.3

[memory]

limits/multithreaded_server/rid_pool_prealloc=20
limits/message_queue/max_size_kb=1024
limits/command_queue/multithreading_queue_size_kb=128

[physics]

common/physics_fps=10
common/physics_jitter_fix=10.0
common/enable_pause_aware_picking=true
3d/default_gravity=3.0
3d/default_gravity_vector=Vector3( 0, -1, 0.05 )
3d/default_linear_damp=-1.0
3d/default_angular_damp=-1.0

[rendering]

threads/thread_model=0
vram_compression/import_bptc=true
vram_compression/import_etc=true
vram_compression/import_pvrtc=true
quality/directional_shadow/size=256
quality/directional_shadow/size.mobile=256
quality/shadow_atlas/size=256
quality/shadow_atlas/size.mobile=256
quality/shadow_atlas/cubemap_size=128
quality/shadow_atlas/quadrant_1_subdiv=1
quality/shadow_atlas/quadrant_2_subdiv=1
quality/shadow_atlas/quadrant_3_subdiv=1
quality/shadows/filter_mode=2
quality/shadows/filter_mode.mobile=2
quality/reflections/texture_array_reflections.mobile=true
quality/reflections/high_quality_ggx.mobile=true
quality/shading/force_vertex_shading=true
quality/shading/force_lambert_over_burley=true
quality/shading/force_blinn_over_ggx=true
quality/filters/use_nearest_mipmap_filter=true
batching/parameters/colored_vertex_format_threshold=0.0
batching/parameters/batch_buffer_size=1024
quality/subsurface_scattering/quality=0
quality/voxel_cone_tracing/high_quality=true
environment/default_clear_color=Color( 0.321569, 0.219608, 0.278431, 1 )
cpu_lightmapper/quality/low_quality_ray_count=1
cpu_lightmapper/quality/medium_quality_ray_count=1
cpu_lightmapper/quality/high_quality_ray_count=1
cpu_lightmapper/quality/ultra_quality_ray_count=1
quality/reflections/atlas_size=957
quality/filters/use_fxaa=true
quality/filters/use_debanding=true
quality/depth/hdr.mobile=true
environment/default_environment="res://default_env.tres"
